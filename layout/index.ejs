<%- partial('_partial/pagehero') %>


<h2>Posts</h2>
<ul class="post-list">
  <% page.posts.each(function(post){ %>
    <li class="post-list-item">
      <a href="<%- url_for(post.path) %>"><%= post.title || 'Untitled' %></a>
      <small class="post-date"><%= date(post.date, 'YYYY-MM-DD') %></small>
    </li>
  <% }) %>
</ul>